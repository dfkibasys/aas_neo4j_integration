{
  "name": "Neo4jSinkConnectorAasEvents",
  "config": {
    "connector.class": "org.eclipse.basyx.kafka.connect.neo4j.Neo4jSinkConnector",
    "tasks.max": "1",
    "executor.threads": "1",
    "batch.size": "1",
    "topics": "aas-events,submodel-events",

    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",

    "errors.tolerance": "all",
    "errors.log.enable": true,
    "errors.deadletterqueue.topic.name": "dlq-aas-events",
    "errors.deadletterqueue.topic.replication.factor": "1",
    "errors.deadletterqueue.context.headers.enable": "true",

    "target.url": "http://neo4j:7474/db/neo4j/tx/commit",
    "headers": "Content-Type:application/json",
    "retry.max.retries": "5",
    "retry.backoff.ms": "2000",
    "http.connection.timeout.ms": "10000",
    "http.read.timeout.ms": "10000"
  }
}
